{
    "python.pythonPath":".venv/bin/python",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true,
        "**/__pycache__": true
    },
    "python.linting.enabled": true,
    "python.autoComplete.addBrackets": true,
    "python.jediEnabled": false,
    "python.analysis.logLevel": "Trace",
}
